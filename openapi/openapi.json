  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ProvenanceRef": {
        "properties": {
          "type": {
            "type": "string",
            "title": "Type"
          },
          "id": {
            "type": "string",
            "title": "Id"
          }
        },
        "additionalProperties": false,
        "type": "object",
        "required": [
          "type",
          "id"
        ],
        "title": "ProvenanceRef"
      },
      "ToolCatalogEntry": {
        "properties": {
          "name": {
            "type": "string",
            "enum": [
              "youtube.likes.list_recent",
              "youtube.likes.search_recent_content",
              "youtube.transcript.get",
              "vault.recipe.save",
              "vault.note.save",
              "bucket.item.add",
              "bucket.item.update",
              "bucket.item.complete",
              "bucket.item.search",
              "bucket.item.recommend",
              "bucket.health.report",
              "memory.create",
              "memory.list",
              "memory.search",
              "memory.delete",
              "memory.undo",
              "reminder.schedule",
              "context.suggest_for_query",
              "digest.weekly_learning.generate",
              "review.routine.generate",
              "recipe.extract_from_transcript",
              "summary.extract_key_ideas",
              "actions.extract_from_notes"
            ],
            "title": "Name"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "write_operation": {
            "type": "boolean",
            "title": "Write Operation"
          },
          "ready_for_use": {
            "type": "boolean",
            "title": "Ready For Use"
          },
          "readiness_note": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Readiness Note"
          }
        },
        "additionalProperties": false,
        "type": "object",
        "required": [
          "name",
          "description",
          "write_operation",
          "ready_for_use"
        ],
        "title": "ToolCatalogEntry"
      },
      "ToolContext": {
        "properties": {
          "timezone": {
            "type": "string",
            "title": "Timezone",
            "default": "Europe/Bucharest"
